<robot name="justina">

	<link name="base_link">
		<visual>
			<origin xyz="0 0 0.065" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.48" radius="0.01"/>
			</geometry>
			<material name="green">
           		<color rgba="0 0.8 0 1"/>
        	</material>
		</visual>
	</link>

	<link name="base_body_link">
		<visual>
			<origin xyz="-0.11 0.0 0.185" rpy="0 0 0"/>
			<geometry>
				<box size="0.5 0.42 0.19"/>
			</geometry>
			<material name="blue">
           		<color rgba="0 0 0.8 1"/>
        	</material>
		</visual>
	</link>

	<link name="torso_link">
		<visual>
			<origin xyz= "-0.0125 0 0.77" rpy="0 0 0"/>
			<geometry>
				<box size="0.045 0.045 0.98"/>
			</geometry>
			<material name="red">
           		<color rgba="0.8 0 0 1"/>
        	</material>
		</visual>
	</link>

	<link name="neck_link">
		<visual>
			<origin xyz= "-0.0125 0 1.409" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.208" radius="0.0175"/>
			</geometry>
			<material name="green">
           		<color rgba="0 0.8 0 1"/>
        	</material>
		</visual>
	</link>

	<link name="neck_m1_link">
		<visual>
			<!-- <origin xyz= "-0.0125 0 1.45" rpy="0 0 0"/> -->
			<origin xyz= "0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.029" radius="0.013"/>
			</geometry>
			<material name="blue">
           		<color rgba="0 0.8 0.8 1"/>
        	</material>
		</visual>
	</link>

	<link name="neck_m2_link">
		<visual>
			<!-- <origin xyz= "-0.0125 0 1.45" rpy="0 0 0"/> -->
			<origin xyz= "0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.0175" radius="0.0145"/>
			</geometry>
			<material name="yellow">
           		<color rgba="0 0.8 0.8 1"/>
        	</material>
		</visual>
	</link>

	<link name="kinect_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.058 0.207 0.04"/>
			</geometry>
			<material name="black">
           		<color rgba="0 0 0 1"/>
        	</material>
		</visual>
	</link>

	<link name="laser_link">
		<visual>
			<origin xyz="0.0325 0 0.725" rpy="0 0 0"/>
			<geometry>
				<box size="0.045 0.05 0.08"/>
			</geometry>
			<material name="black">
           		<color rgba="0 0 0 1"/>
        	</material>
		</visual>
	</link>

	<link name="wheel_left">
		<visual>
			<origin xyz="0 -0.24 0.065" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.04" radius="0.06"/>
			</geometry>
			<material name="gray">
      			<color rgba="0 0 0 1" />
    		</material>
		</visual>
	</link>

	<link name="wheel_right">
		<visual>
			<origin xyz="0 0.24 0.065" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.04" radius="0.06"/>
			</geometry>
			<material name="black">
      			<color rgba="0 0 0 1" />
    		</material>
		</visual>
	</link>

	<link name="shoulders_link">
		<visual>
			<origin xyz= "-0.0125 0 1.2825" rpy="0 0 0"/>
			<geometry>
				<box size="0.045 0.43 0.045"/>
			</geometry>
			<material name="red">
           		<color rgba="0.8 0 0 1"/>
        	</material>
		</visual>
	</link>



	<joint name="base_connect" type="fixed">
  		<origin xyz="0 0 0" />
  	
  		<parent link="base_link"/>
  		<child link="base_body_link"/>
	</joint>

	<joint name="wheel_left_connect" type="fixed">
  		<origin xyz="0 0 0" />
  	
  		<parent link="base_body_link"/>
  		<child link="wheel_left"/>
	</joint>

	<joint name="wheel_right_connect" type="fixed">
  		<origin xyz="0 0 0" /> 
  		
  		<parent link="base_body_link"/>
  		<child link="wheel_right"/>
	</joint>

	<joint name="torso_connect" type="fixed">
  		<origin xyz="0 0 0" /> 
  		
  		<parent link="base_body_link"/>
  		<child link="torso_link"/>
	</joint>

	<joint name="laser_connect" type="fixed">
  		<origin xyz="0 0 0" />
  		<parent link="torso_link"/>
  		<child link="laser_link"/>
	</joint>

	<joint name="shoulders_connect" type="fixed">
  		<origin xyz="0 0 0" />
  		<parent link="torso_link"/>
  		<child link="shoulders_link"/>
	</joint>

	<joint name="neck_connect" type="fixed">
  		<origin xyz="0 0 0" />
  		<parent link="shoulders_link"/>
  		<child link="neck_link"/>
	</joint>

	<joint name="pan_connect" type="revolute">
  		
  		<parent link="neck_link"/>
  		<child link="neck_m1_link"/>
  		<axis xyz="0 0 1" />
  		<limit upper="0" lower="0" effort="0" velocity="0" />
  		<origin xyz="-0.0125 0 1.5275" />
	</joint>
	
	<joint name="tilt_connect" type="revolute">
  		<parent link="neck_m1_link"/>
  		<child link="neck_m2_link"/>
  		<axis xyz="0 1 0" />
  		<limit upper="0" lower="0" effort="0" velocity="0" />
  		<origin xyz="0 0 0" />
	</joint>

	<joint name="kinect_connect" type="fixed">
  		<origin xyz="0.06 0 0.075" />
  		<parent link="neck_m2_link"/>
  		<child link="kinect_link"/>
	</joint>

</robot>
