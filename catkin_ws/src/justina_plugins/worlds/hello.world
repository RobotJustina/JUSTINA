<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <model name="box">
	<pose>0 0 0.5 0 0 0</pose>
	<link name="link">
	<collision name="collision">
	<geometry>
		<box>
			<size>1 1 1</size>
		</box>
	</geometry>
	</collision>

	<visual name="visual">
		<geometry>
			<box>
				<size>1 1 1</size>
			</box>
		</geometry>
	</visual>
	</link>

    <!--<include>
      <uri>model://gas_station</uri>
      <name>gas_station</name>
      <pose>-2.0 7.0 0 0 0 0</pose>
    </include> -->

    <!-- reference to your plugin -->
    <plugin name="model_push" filename="libmodel_push.so"/>
    </model>
  </world>
</sdf>
