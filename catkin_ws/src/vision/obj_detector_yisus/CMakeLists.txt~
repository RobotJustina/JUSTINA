cmake_minimum_required(VERSION 2.8.3)
project(obj_detector_yisus)

find_package(catkin REQUIRED COMPONENTS
  roscpp
  rospy
  sensor_msgs
  std_msgs
  vision_msgs
)

catkin_package(
)

include_directories(
  ${catkin_INCLUDE_DIRS}
)

add_executable(obj_detector_yisus_node src/obj_detector_yisus.cpp)

target_link_libraries(obj_detector_yisus_node
   ${catkin_LIBRARIES}
)

